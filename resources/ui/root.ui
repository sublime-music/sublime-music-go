<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.2 -->
<interface>
  <!-- interface-name root.ui -->
  <requires lib="gtk" version="4.6" />
  <requires lib="libadwaita" version="1.1" />
  <object class="GtkBox" id="root-box">
    <property name="orientation">vertical</property>
    <property name="vexpand">true</property>
    <child>
      <object class="AdwHeaderBar">
        <child type="start">
          <object class="GtkMenuButton" id="SearchButton">
            <property name="icon-name">edit-find-symbolic</property>
          </object>
        </child>
        <child type="title">
          <object class="AdwViewSwitcherTitle" id="switcher-title">
            <property name="stack">root-stack</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwViewStack" id="root-stack">
        <property name="vexpand">true</property>
        <child>
          <object class="AdwViewStackPage">
            <property name="icon-name">library-music-symbolic</property>
            <property name="name">albums</property>
            <property name="title">Albums</property>
            <property name="child">
              <object class="AdwStatusPage">
                <property name="description">All of your albums</property>
                <property name="icon-name">library-music-symbolic</property>
                <property name="title">Albums</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwViewStackPage">
            <property name="icon-name">library-artists-symbolic</property>
            <property name="name">artists</property>
            <property name="title">Artists</property>
            <property name="child">
              <object class="AdwStatusPage">
                <property name="description">All of your artists</property>
                <property name="icon-name">library-artists-symbolic</property>
                <property name="title">Artists</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwViewStackPage">
            <property name="icon-name">columns-symbolic</property>
            <property name="name">browse</property>
            <property name="title">Browse</property>
            <property name="child">
              <object class="AdwStatusPage">
                <property name="description">Browse songs</property>
                <property name="icon-name">columns-symbolic</property>
                <property name="title">Browse</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwViewStackPage">
            <property name="icon-name">playlist-symbolic</property>
            <property name="name">playlists</property>
            <property name="title">Playlists</property>
            <property name="child">
              <object class="AdwStatusPage">
                <property name="description">All of your playlists</property>
                <property name="icon-name">playlist-symbolic</property>
                <property name="title">Playlists</property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwViewSwitcherBar">
        <property name="stack">root-stack</property>
        <binding name="reveal">
          <lookup name="title-visible">switcher-title</lookup>
        </binding>
      </object>
    </child>
  </object>
</interface>
